config 'global'
    option  anon_swap       '0'
    option  anon_mount      '0'
    option  auto_swap       '1'
    option  auto_mount      '0'
    option  delay_root      '5'
    option  check_fs        '1'

config 'mount'
    option  target  '/mnt/sdcard'
    option  device  '/dev/mmcblk0p1'
    option  options 'noatime,nodiratime,data=writeback'
    option  enabled '1'
